{
  "name": "database-query-tool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "start": "PORT=3010 next start",
    "lint": "eslint",
    "pm2:dev": "pm2 start ecosystem.config.js --only dev",
    "pm2:prod": "pm2 start ecosystem.config.js --only prod",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:list": "node scripts/pmlist.js",
    "pm2:clear": "bash scripts/clear-ports.sh",
    "test": "playwright test",
    "test:file": "playwright test",
    "test:ui": "xvfb-run -a pnpm exec playwright test --ui --ui-host=0.0.0.0",
    "test:ui:prod": "xvfb-run -a pnpm exec playwright test --ui --ui-host=0.0.0.0",
    "test:prod": "playwright test",
    "test:verbose": "playwright test --reporter=list",
    "test:watch": "PWTEST_WATCH=1 playwright test --reporter=list",
    "test:debug": "xvfb-run --auto-servernum --server-args='-screen 0 1920x1080x24' playwright test --debug --workers=1",
    "test:headed": "xvfb-run --auto-servernum --server-args='-screen 0 1920x1080x24' playwright test --headed",
    "test:traces": "playwright test --reporter=html",
    "test:report": "playwright show-report",
    "test:report-server": "cd playwright-report && python3 -m http.server 9326 --bind 0.0.0.0",
    "test:review": "playwright test --reporter=html && npx playwright show-report",
    "test:project": "playwright test --project=chromium"
  },
  "dependencies": {
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/lang-sql": "^6.10.0",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@ducanh2912/next-pwa": "^10.2.9",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.93.2",
    "@uiw/react-codemirror": "^4.25.4",
    "comlink": "^4.4.2",
    "next": "16.1.5",
    "pyodide": "^0.29.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sql.js": "^1.13.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sql.js": "^1.4.9",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.5",
    "pm2": "^6.0.14",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
